{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "outDir": "./built",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "./*"
      ],
    },
    "types": [
      "node",
      "@wdio/sync",
      "@wdio/jasmine-framework"
    ]
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}